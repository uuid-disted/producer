{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Launch Package",
          "type": "go",
          "request": "launch",
          "mode": "auto",
          "program": "${workspaceFolder}",
          "env": {},
          "args": []
      },
      {
          "name": "Launch File",
          "type": "go",
          "request": "launch",
          "mode": "auto",
          "program": "${file}",
          "env": {},
          "args": []
      },
      {
          "name": "Launch Tests in Package",
          "type": "go",
          "request": "launch",
          "mode": "test",
          "program": "${workspaceFolder}",
          "env": {},
          "args": []
      },
      {
          "name": "Launch Tests in File",
          "type": "go",
          "request": "launch",
          "mode": "test",
          "program": "${fileDirname}",
          "env": {},
          "args": []
      },
      {
          "name": "Attach to Process",
          "type": "go",
          "request": "attach",
          "processId": "${command:pickProcess}"
      }
  ]
}
